{
  "version": 2,
  "routes": [
    {
      "src": "/$",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/custom.xaml",
      "headers": {
        "Content-Disposition": "attachment; filename=\"custom.xaml\"",
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    }
  ],
  "builds": [
    {
      "src": "*.{html,xaml}",
      "use": "@vercel/static"
    }
  ],
  "cleanUrls": true
}
