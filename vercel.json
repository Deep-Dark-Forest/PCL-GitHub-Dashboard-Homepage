{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "Custom.xaml",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "index.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "^/custom\\.xaml$",
      "headers": {
        "Content-Disposition": "attachment; filename=\"Custom.xaml\"",
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    }
  ]
}
